<Window x:Class="Cake.Cehha"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cake"
        mc:Ignorable="d"
        Title="Цеха" Height="450" Width="800" Background="#FFFF9933">
    <Grid>
        <Label Content="Выбрать план цеха" HorizontalAlignment="Left" Margin="591,10,0,0" VerticalAlignment="Top" Width="120" FontFamily="Comic Sans MS"/>
        <ComboBox x:Name="ComboCehha" HorizontalAlignment="Left" Margin="591,36,0,0" VerticalAlignment="Top" Width="120" />
        <StackPanel HorizontalAlignment="Left" Height="411" VerticalAlignment="Top" Width="326">
            <Image Height="57" Margin="0,0,248,0" OpacityMask="#FFD17F7F" Cursor="Arrow" AllowDrop="True" Source="Logo_Monochrome.png"/>
        </StackPanel>
        <Button Content="+" HorizontalAlignment="Left" Margin="765,99,0,0" VerticalAlignment="Top" Width="21"/>
        <Button Content="-" HorizontalAlignment="Left" Margin="765,120,0,0" VerticalAlignment="Top" Width="21" Height="20"/>
    </Grid>
</Window>
