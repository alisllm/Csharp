        <Label Content="Имя" HorizontalAlignment="Left" Margin="169,123,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.518" Width="65"/>
        <Label Content="Пол" HorizontalAlignment="Left" Margin="169,174,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.518,0.5"/>
