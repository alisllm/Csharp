        <TextBox x:Name="otec" HorizontalAlignment="Left" Height="23" Margin="217,150,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="pol" HorizontalAlignment="Left" Height="23" Margin="217,177,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="login2" HorizontalAlignment="Left" Height="23" Margin="217,229,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="pass2" HorizontalAlignment="Left" Height="23" Margin="217,259,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
