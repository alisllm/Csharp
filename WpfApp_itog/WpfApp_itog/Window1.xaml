        xmlns:local="clr-namespace:WpfApp_itog"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid RenderTransformOrigin="0.589,0.355">
        <Button Content="Зарегистрироваться " HorizontalAlignment="Left" Margin="123,353,0,0" VerticalAlignment="Top" Width="121" Click="Button_Click_1"/>
        <Label Content="Дата рождения" HorizontalAlignment="Left" Margin="117,197,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.518" Width="95"/>
        <Label HorizontalAlignment="Left" Margin="169,187,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.518" Width="65"/>
        <Label Content="Логин" HorizontalAlignment="Left" Margin="167,229,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.504,0.518" Width="65"/>
