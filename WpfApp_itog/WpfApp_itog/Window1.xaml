        <TextBox x:Name="pathphoto" HorizontalAlignment="Left" Height="16" Margin="432,247,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="234" TextChanged="Pathphoto_TextChanged"/>
        <Button Content="Отмена" HorizontalAlignment="Left" Margin="249,353,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="680,231,0,0" VerticalAlignment="Top" Width="75" Height="32" Click="Button_Click_2"/>
        <Image x:Name="photo" HorizontalAlignment="Left" Height="130" Margin="466,92,0,0" VerticalAlignment="Top" Width="137"/>
        <Label Content="Контактный номер" HorizontalAlignment="Left" Margin="342,286,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.518,0.5"/>
        <TextBox x:Name="nomerr" HorizontalAlignment="Left" Height="23" Margin="452,289,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
